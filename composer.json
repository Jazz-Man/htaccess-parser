{
  "name": "jazzman/htaccess-parser",
  "description": "A .htaccess parser and validator implemented in PHP",
  "keywords": [".htaccess", "htaccess"],
  "homepage": "https://github.com/tivie/php-htaccess-parser",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Estev√£o Soares dos Santos",
      "email": "estevao@soares-dos-santos.com"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "jazzman/php-cs-fixer-rules": "^0.2.0",
    "phpdocumentor/phpdocumentor": "^3.1",
    "phpunit/phpunit": "^10.3"
  },
  "autoload": {
    "psr-4": {
      "JazzMan\\HtaccessParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JazzMan\\HtaccessParserTest\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "classmap-authoritative": true,
    "apcu-autoloader": true,
    "allow-plugins": {
      "phpdocumentor/unified-asset-installer": true,
      "symfony/flex": true
    }
  }
}
